{
  "name": "grpc-gcp",
  "version": "0.3.2",
  "description": "Extension for supporting Google Cloud Platform specific features for gRPC.",
  "main": "build/src/index.js",
  "scripts": {
    "build": "./codegen.sh && tsc && cp -r src/generated build/src/",
    "test-codegen": "./test/codegen.sh",
    "presystem-test": "npm run test-codegen",
    "system-test": "c8 mocha test/integration/*.js --reporter spec --timeout 10000",
    "test": "c8 mocha test/unit/*.js --reporter spec",
    "eslint": "./node_modules/.bin/eslint test/integration",
    "lint": "gts check",
    "fix": "gts fix",
    "prepare": "npm run build",
    "prettier": "prettier --write src/*.ts test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleCloudPlatform/grpc-gcp-node.git"
  },
  "keywords": [
    "google",
    "grpc",
    "cloud",
    "gcp"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "types": "build/src/index.d.ts",
  "bugs": {
    "url": "https://github.com/GoogleCloudPlatform/grpc-gcp-node/issues"
  },
  "homepage": "https://github.com/GoogleCloudPlatform/grpc-gcp-node#readme",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@grpc/grpc-js": "~1.1.0"
  },
  "devDependencies": {
    "@grpc/proto-loader": "^0.5.4",
    "c8": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "google-auth-library": "^6.0.0",
    "google-gax": "^2.3.0",
    "google-protobuf": "^3.11.4",
    "grpc": "^1.24.2",
    "grpc-tools": "^1.8.1",
    "gts": "^0.9.0",
    "mocha": "^7.1.1",
    "prettier": "^2.0.5",
    "typescript": "~3.8.3"
  },
  "files": [
    "build/src"
  ]
}
